<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark"
				xmlns:mx="library://ns.adobe.com/flex/mx">
	<s:BorderContainer  borderWeight="1" width="100" height="66" backgroundAlpha="0.0"  >
		<s:layout>
			<s:VerticalLayout paddingTop="0" paddingLeft="0" paddingRight="0" paddingBottom="0" horizontalAlign="center"/>
		</s:layout>
		<s:Scroller verticalScrollPolicy="off">
		<s:Group width="90"  height="66">
			<s:layout>
				<s:VerticalLayout horizontalAlign="center" verticalAlign="middle"/>
			</s:layout>
			<s:Rect>
				<s:stroke>
					<s:SolidColorStroke color="#000000" weight="1"/>
				</s:stroke>
			</s:Rect>
			<mx:Image source="{data.thumb}" id="image" scaleContent="true"/>
		</s:Group >
		</s:Scroller>
	</s:BorderContainer>
</s:ItemRenderer>
